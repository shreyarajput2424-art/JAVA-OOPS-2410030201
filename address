public class PrintAddress {
    public static void main(String[] args) {
        
        
        System.out.println("D Block 100\nGamma 1\nGreater Noida\nU.P.");
        
        System.out.println();

   
        System.out.println("D Block 100");
        System.out.println("Gamma 1");
        System.out.println("Greater Noida");
        System.out.println("U.P.");
    }
}
